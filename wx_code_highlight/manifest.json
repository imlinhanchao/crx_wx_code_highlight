{
  "name": "微信公众号代码高亮插入器",
  "manifest_version": 2,
  "version": "1.2",
  "description": "微信公众号代码高亮插入器",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [{
      "matches": ["https://mp.weixin.qq.com/*"],
      "css": ["highlight.min.css", "base.css"],
      "js": ["weixin.js", "highlight.min.js", "jquery.min.js"]
    }]
}
